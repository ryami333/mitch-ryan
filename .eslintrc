{
	"parser": "@typescript-eslint/parser",
	"extends": [
		"plugin:react/recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier",
		"prettier/react",
		"prettier/@typescript-eslint"
	],
	"plugins": ["@typescript-eslint", "react", "prettier"],
	"env": {
		"es6": true,
		"browser": true
	},
	"overrides": [
		{
			"files": ["*.js"],
			"rules": {
				"@typescript-eslint/no-var-requires": "off"
			}
		}
	]
}
